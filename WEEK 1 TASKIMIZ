alish_qiymeti = input("alish qiymetini gir qaqashim: ")
satish_qiymeti = input("satish qiymetini gir qaqashim: ")

if (int(satish_qiymeti) - int(alish_qiymeti))<0:
    print("sen poxdasan, ziyanin da ", (int(satish_qiymeti) - int(alish_qiymeti)), " manatdi")
elif (int(satish_qiymeti) - int(alish_qiymeti))>0:
     print("QAQASHIM QAZANIT AXI, OZDE ", ((int(satish_qiymeti) - int(alish_qiymeti)), "MANATTT BLETTTT"))
else:
    print("QAQASHIM QOYUNDU, POX DA QAZANNAMIYIB")
